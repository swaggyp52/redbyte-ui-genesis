    "@playwright/test": "^1.49.1",
    "@vitejs/plugin-react-swc": "^4.0.0",
    "eslint": "^9.14.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.11",
    "jsdom": "^26.0.0",
    "prettier": "^3.3.3",
    "tailwindcss": "^3.4.16",
